LINK 800
COPY 1 X
MARK LO
MAKE
MULI X -1 F
@REP 4
COPY @{-3,2} X
SEEK -1
TEST X = F
TJMP NOT@{0,1}
COPY 10 T
REPL AGENT
MARK NOT@{0,1}
@END
WIPE
COPY #NERV T
HOST M
COPY 52 T
MARK SLEEP
SUBI T 1 T
TJMP SLEEP
MARK RESP
HOST T
COPY M X
TEST X = T
TJMP ANS
COPY X M
COPY 14 T
MARK RSL
SUBI T 1 T
TJMP RSL
JUMP RESP

MARK ANS
COPY #NERV M
HALT

MARK AGENT
LINK X
JUMP LO